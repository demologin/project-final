            </column>
            <column name="STARTPOINT" type="timestamp"/>
            <column name="ENDPOINT" type="timestamp"/>
            <column name="TITLE" type="varchar(1024)">
                <constraints nullable="false"/>
            </column>
            <column name="PROJECT_ID" type="bigint">
                <constraints foreignKeyName="FK_SPRINT_PROJECT"
                             references="PROJECT (ID)"
                             deleteCascade="true"/>
            </column>
        </createTable>
    </changeSet>
    <changeSet id="18" author="agribanov">
        <createTable tableName="REFERENCE">
            <column name="ID" type="bigserial">
                <constraints primaryKey="true"/>
            </column>
            <column name="CODE" type="varchar(32)">
                <constraints nullable="false"
                             uniqueConstraintName="UK_REFERENCE_REF_TYPE_CODE"
                             unique="true"/>
            </column>
            <column name="REF_TYPE" type="smallint">
                <constraints nullable="false"
                             uniqueConstraintName="UK_REFERENCE_REF_TYPE_CODE"
                             unique="true"/>
            </column>
            <column name="ENDPOINT" type="timestamp"/>
            <column name="STARTPOINT" type="timestamp"/>
            <column name="TITLE" type="varchar(1024)">
                <constraints nullable="false"/>
            </column>
            <column name="AUX" type="varchar"/>
        </createTable>
    </changeSet>
    <changeSet id="19" author="agribanov">
        <createTable tableName="USERS">
            <column name="ID" type="bigserial">
