            </column>
            <column name="USER_ID" type="bigint">
                <constraints nullable="false"
                             foreignKeyName="FK_USER_BELONG"
                             references="USERS (ID)"
                             uniqueConstraintName="UK_USER_BELONG_OBJECT_USER_ROLE"
                             unique="true"
                />
            </column>
            <column name="USER_TYPE_CODE" type="varchar(32)">
                <constraints nullable="false"/>
            </column>
            <column name="TASK_USER_ROLE" type="smallint">
                <constraints nullable="false"
