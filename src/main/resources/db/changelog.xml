                             uniqueConstraintName="UK_USERS_DISPLAY_NAME"
                             unique="true"/>
            </column>
            <column name="EMAIL" type="varchar(128)">
                <constraints nullable="false"
                             uniqueConstraintName="UK_USERS_EMAIL"
                             unique="true"/>
            </column>
            <column name="FIRST_NAME" type="varchar(32)">
                <constraints nullable="false"/>
            </column>
            <column name="LAST_NAME" type="varchar(32)"/>
            <column name="PASSWORD" type="varchar(128)">
                <constraints nullable="false"/>
            </column>
            <column name="ENDPOINT" type="timestamp"/>
            <column name="STARTPOINT" type="timestamp"/>
        </createTable>
    </changeSet>
    <changeSet id="20" author="agribanov">
        <createTable tableName="PROFILE">
            <column name="ID" type="bigint">
                <constraints primaryKey="true"
                             foreignKeyName="FK_PROFILE_USERS"
                             references="USERS (ID)"
                             deleteCascade="true"/>
            </column>
            <column name="LAST_LOGIN" type="timestamp"/>
            <column name="LAST_FAILED_LOGIN" type="timestamp"/>
            <column name="MAIL_NOTIFICATIONS" type="bigint"/>
        </createTable>
    </changeSet>
    <changeSet id="21" author="agribanov">
        <createTable tableName="CONTACT">
            <column name="ID" type="bigint">
                <constraints nullable="false"
                             primaryKey="true"
                             foreignKeyName="FK_CONTACT_PROFILE"
                             references="PROFILE (ID)"
                             deleteCascade="true"/>
            </column>
            <column name="CODE" type="varchar(32)">
                <constraints nullable="false"
                             primaryKey="true"/>
            </column>
            <column name="VALUE" type="varchar(256)">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
    <changeSet id="22" author="agribanov">
        <createTable tableName="TASK">
            <column name="ID" type="bigserial">
                <constraints primaryKey="true"/>
            </column>
            <column name="TITLE" type="varchar(1024)">
                <constraints nullable="false"/>
            </column>
            <column name="DESCRIPTION" type="varchar(4096)">
                <constraints nullable="false"/>
            </column>
            <column name="TYPE_CODE" type="varchar(32)">
                <constraints nullable="false"/>
            </column>
            <column name="STATUS_CODE" type="varchar(32)">
                <constraints nullable="false"/>
            </column>
            <column name="PRIORITY_CODE" type="varchar(32)">
                <constraints nullable="false"/>
            </column>
            <column name="ESTIMATE" type="integer"/>
            <column name="UPDATED" type="timestamp"/>
            <column name="PROJECT_ID" type="bigint">
                <constraints nullable="false"
                             foreignKeyName="FK_TASK_PROJECT"
                             references="PROJECT (ID)"
                             deleteCascade="true"/>
            </column>
            <column name="SPRINT_ID" type="bigint"/>
            <column name="PARENT_ID" type="bigint">
                <constraints foreignKeyName="FK_TASK_PARENT_TASK"
                             references="TASK (ID)"
                             deleteCascade="true"/>
            </column>
            <column name="STARTPOINT" type="timestamp"/>
            <column name="ENDPOINT" type="timestamp"/>
        </createTable>
        <addForeignKeyConstraint
                baseTableName="TASK"
                baseColumnNames="SPRINT_ID"
                constraintName="FK_TASK_SPRINT"
                referencedTableName="SPRINT"
                referencedColumnNames="ID"
                onDelete="SET NULL"/>
    </changeSet>
    <changeSet id="23" author="agribanov">
        <createTable tableName="ACTIVITY">
            <column name="ID" type="bigserial">
                <constraints primaryKey="true"/>
            </column>
            <column name="AUTHOR_ID" type="bigint">
                <constraints nullable="false"
                             foreignKeyName="FK_ACTIVITY_USERS"
                             references="USERS (ID)"/>
            </column>
            <column name="TASK_ID" type="bigint">
                <constraints nullable="false"
                             foreignKeyName="FK_ACTIVITY_TASK"
                             references="TASK (ID)"
                             deleteCascade="true"/>
            </column>
            <column name="UPDATED" type="timestamp"/>
            <column name="COMMENT" type="varchar(4096)"/>
            <!-- history of task field change -->
            <column name="TITLE" type="varchar(1024)"/>
            <column name="DESCRIPTION" type="varchar(4096)"/>
            <column name="ESTIMATE" type="integer"/>
            <column name="TYPE_CODE" type="varchar(32)"/>
            <column name="STATUS_CODE" type="varchar(32)"/>
            <column name="PRIORITY_CODE" type="varchar(32)"/>
        </createTable>
    </changeSet>
    <changeSet id="24" author="agribanov">
        <createTable tableName="TASK_TAG">
            <column name="TASK_ID" type="bigint">
                <constraints nullable="false"
                             uniqueConstraintName="UK_TASK_TAG"
                             unique="true"
                             foreignKeyName="FK_TASK_TAG"
                             references="TASK (ID)"
                             deleteCascade="true"/>
            </column>
            <column name="TAG" type="varchar(32)">
                <constraints nullable="false"
                             uniqueConstraintName="UK_TASK_TAG"
                             unique="true"/>
            </column>
        </createTable>
    </changeSet>
